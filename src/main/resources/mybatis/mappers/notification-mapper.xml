<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.fundingflex.mybatis.mapper.notification.NotificationMapper">

	<!-- 카테고리 모두 조회 -->
	<insert id="insertNotification">
		INSERT INTO notification (fc_id, user_id, message)
		  VALUES (#{fcId}, #{userId}, #{message})
	</insert>

</mapper>