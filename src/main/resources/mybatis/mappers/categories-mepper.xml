<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.fundingflex.mybatismapper.repository.CategoriesRepository">
	
	<!-- 카테고리 조회 -->
	<select id="selectAllCategories" resultType="com.fundingflex.category.domain.dto.dto.CategoriesDto">
        SELECT category_id 		categoryId
        	 , category_name 	categoryName
          FROM categories
    </select>

</mapper>